{
  "ENABLE_PARALLEL_PROCESSING": true,
  "USE_OPTIMIZED_FEATURES": true,
  "BATCH_SIZE_OPTIMIZATION": true,
  "ENABLE_DIAGNOSTIC_LOGGING": true,
  "MEMORY_REALLOCATION": true,
  "MAX_WORKERS": 6,
  "OPTIMIZED_BATCH_SIZE": 1000,
  "LATENCY_TARGET_MS": 2000,
  "MAX_MEMORY_GB": 6.0,
  "MAX_CPU_PERCENT": 50.0,
  "LOG_PERFORMANCE_METRICS": true,
  "ALERT_ON_REGRESSION": true,
  "feature_flags": {
    "parallel_processing": {
      "enabled": true,
      "fallback_to_sequential": true,
      "timeout_seconds": 30
    },
    "feature_reduction": {
      "enabled": true,
      "target_features": 30,
      "remove_duplicates": true
    },
    "memory_optimization": {
      "enabled": true,
      "reclaim_jax": true,
      "reclaim_ray": true,
      "target_recovery_mb": 1800
    },
    "jvm_optimization": {
      "enabled": true,
      "gc_threads": 2,
      "heap_size_mb": 500
    },
    "diagnostic_logging": {
      "enabled": true,
      "log_level": "INFO",
      "performance_tracking": true,
      "correlation_ids": true,
      "auto_analysis": true
    }
  },
  "rollback_triggers": {
    "max_latency_ms": 5000,
    "max_memory_gb": 6.5,
    "max_cpu_percent": 80,
    "max_error_rate": 0.1
  }
}